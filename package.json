    "bugs": {
        "url": "https://github.com/obernardovieira/impactMarket/issues"
    },
    "homepage": "https://github.com/obernardovieira/impactMarket#readme",
    "dependencies": {
        "@celo/contractkit": "0.2.14",
        "@openzeppelin/contracts": "2.5.0"
    },
    "devDependencies": {
        "@celo/ganache-cli": "6.6.1",
        "@openzeppelin/test-helpers": "^0.5.5",
        "@types/bignumber.js": "5.0.0",
        "@types/chai": "4.2.10",
        "@types/mocha": "7.0.2",
        "@types/node": "12.12.31",
        "bignumber.js": "9.0.0",
        "chai": "4.2.0",
