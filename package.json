        "ts-node": "8.6.2",
        "typechain": "1.0.5",
        "typechain-target-truffle": "1.0.2",
        "typescript": "3.8.3",
        "solidity-coverage": "0.7.1",
        "coveralls": "3.0.11",
        "@typescript-eslint/eslint-plugin": "2.19.0",
        "@typescript-eslint/parser": "2.19.0",
        "eslint": "6.8.0"
